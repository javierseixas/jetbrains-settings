<templateSet group="MapStruct">
  <template name="mp" value="$OUTPUT_CLASS$ $INPUT_CLASS$To$OUTPUT_CLASS$($INPUT_CLASS$ $INPUT_VAR$);" description="Creates a mapping method" toReformat="false" toShortenFQNames="true">
    <variable name="OUTPUT_CLASS" expression="" defaultValue="&quot;OutputClass&quot;" alwaysStopAt="true" />
    <variable name="INPUT_CLASS" expression="" defaultValue="&quot;InputClass&quot;" alwaysStopAt="true" />
    <variable name="INPUT_VAR" expression="camelCase(INPUT_CLASS)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>